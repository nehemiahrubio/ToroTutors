{
  "name": "torotutor",
  "version": "1.2.0",
  "description": "Social Network for CSUDH students to find tutors",
  "main": "server.js",
  "engines": {
    "node": "20.11.0"
  },
  "scripts": {
    "test": "mocha --timeout 10000",
    "start": "node server.js",
    "build": "npm run build --prefix client",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dropdb": "node drop.db.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TutorUP/TutorUP-App.git"
  },
  "keywords": [
    "react",
    "express",
    "tutor",
    "mongo",
    "node"
  ],
  "author": "Danh Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TutorUP/TutorUP-App/issues"
  },
  "homepage": "https://github.com/TutorUP/TutorUP-App#readme",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@material-ui/core": "^4.12.4",
    "@sendgrid/mail": "^8.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^8.4.1",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "heroku-logger": "^0.0.2",
    "hsts": "^2.2.0",
    "i": "^0.3.6",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.4.0",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sinon": "^7.2.2",
    "sinon-mongoose": "^2.2.1",
    "supertest": "^3.4.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.23.4",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0"
  }
}
